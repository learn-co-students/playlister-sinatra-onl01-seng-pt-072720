<h1>Create a new Song</h1>

<form action="/songs" method="POST">
  <label>Name:</label><input id="Name" type="text" name="song[name]" ><br>
  <label>Artist:</label><input type="text" name="artist[name]" id="Artist Name"><br>
  <label>Genres:</label><br>
  <% @genres.each do |genre| %>
    <input id="<%= genre.name %>" type="checkbox" name="genres[]" value="<%= genre.id %>"><%= genre.name %><br>
  <% end %>
  <br><br>
  <input type="submit" value="Create Song">
</form>